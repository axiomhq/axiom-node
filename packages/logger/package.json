{
    "name": "axiom-js-logger",
    "description": "The official NodeJS bindings for the Axiom API",
    "version": "0.10.0",
    "author": "Axiom, Inc.",
    "license": "MIT",
    "contributors": [
        "Lukas Malkmus <lukas@axiom.co>",
        "Islam Schehata <islam@axiom.co>",
        "Arne Bahlo <arne@axiom.co>"
    ],
    "engines": {
        "node": ">=14"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "lint": "eslint '{lib,examples}/**/*.{js,ts}' --quiet --fix",
        "prepublish": "npm run build",
        "test": "mocha -r ts-node/register tests/unit/**/*.test.ts",
        "cover": "nyc -r text -e .ts -x 'tests/unit/**/*.test.ts' npm run test",
        "integration": "mocha -r ts-node/register tests/integration/**/*.test.ts ----timeout 10000"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/axiomhq/axiom-js.git"
    },
    "keywords": [
        "axiom",
        "api",
        "rest",
        "client"
    ],
    "bugs": {
        "url": "https://github.com/axiomhq/axiom-js/issues"
    },
    "homepage": "https://github.com/axiomhq/axiom-js",
    "dependencies": {
        "axiom-js": "file:../axim-js"
    },
    "devDependencies": {
        "@types/chai": "^4.3.1",
        "@types/mocha": "^9.1.1",
        "@types/node": "18.11.9",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.6.2",
        "ts-node": "^10.8.1",
        "typescript": "^4.9.3"
    }
}
